    "import math\n",
    "\n",
    "try:\n",
    "    num=int(input(\"Input a number\"))\n",
    "\n",
    "    #compute the factorial of the user input value\n",
    "    factorial_num=math.factorial(num)\n",
    "except ValueError:\n",
    "    print(\"Enter only positive value Eg.:1,3,6\")\n",
    "    \n",
    "else:\n",
    "    print(f\"The factorial of {num} is {factorial_num}\")"
   ]
  }
